CC = cc
CCC = CC
CXX = CC
BASICOPTS = -xO3 -s -misalign -xarch=generic64 -xcode=abs64
CFLAGS = $(BASICOPTS) -xCC
CCFLAGS = $(BASICOPTS)
CXXFLAGS = $(BASICOPTS)
CCADMIN = CCadmin -clean

TARGETDIR_libindigo.a=sparc64static
TARGETDIR_libindigo.so=sparc64shared

USERLIBS_libindigo.so =  ../layout/sparc64/liblayout.a ../reaction/sparc64/libreaction.a ../molecule/sparc64/libmolecule.a ../graph/sparc64/libgraph.a ../tinyxml/sparc64/libtinyxml.a -lrt -lz
DEPLIBS_libindigo.so  =  ../layout/sparc64/liblayout.a ../reaction/sparc64/libreaction.a ../molecule/sparc64/libmolecule.a ../graph/sparc64/libgraph.a ../tinyxml/sparc64/libtinyxml.a

include Makefile.sun.common
